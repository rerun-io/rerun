include "arrow/attributes.fbs";
include "fbs/attributes.fbs";

namespace rerun.datatypes;

// ---

table U8Tensor (order: 100, transparent) {
  data: [ubyte] (order: 100);
}

table U16Tensor (order: 100, transparent) {
  data: [ushort] (order: 100);
}


/// Storage for a `Tensor`
union TensorData (
  order: 100,
  "attr.rust.derive": "PartialEq"
) {
  U8: U8Tensor (transparent, order:100),
  U16: U16Tensor (transparent, order:200),
}
