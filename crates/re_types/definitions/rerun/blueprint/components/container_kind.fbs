include "arrow/attributes.fbs";
include "python/attributes.fbs";
include "rust/attributes.fbs";

include "rerun/datatypes.fbs";
include "rerun/attributes.fbs";

namespace rerun.blueprint.components;


// TODO(#3384)
/*
enum ContainerKind: byte {
    Tabs = 1
    Horizontal = 2,
    Vertical = 3,
    Grid = 4
}
