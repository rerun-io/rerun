// ---

/// The kind of a blueprint container.
table ContainerKind (
    "attr.rerun.scope": "blueprint",
    "attr.docs.unreleased",
    "attr.rust.derive": "Default, PartialEq, Eq, PartialOrd, Ord"
) {
    /// Which kind of container this is.
    ///
    /// Allowed values:
    ///  - Tabs = 1
    ///  - Horizontal = 2
    ///  - Vertical = 3
    ///  - Grid = 4
    kind: ubyte (order: 100);
}
