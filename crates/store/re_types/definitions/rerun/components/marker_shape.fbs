include "rerun/attributes.fbs";

namespace rerun.components;

/// The visual appearance of a point in e.g. a 2D plot.
enum MarkerShape: byte
 {
    /// `⏺`
    Circle (default),

    /// `◆`
    Diamond,

    /// `◼️`
    Square,

    /// `x`
    Cross,

    /// `+`
    Plus,

    /// `▲`
    Up,

    /// `▼`
    Down,

    /// `◀`
    Left,

    /// `▶`
    Right,

    /// `*`
    Asterisk,
}
