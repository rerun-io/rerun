namespace rerun.blueprint.datatypes;

/// Defines a force that pulls nodes towards an `y` position.
struct ForcePositionY (
    "attr.rust.derive": "Default, Clone, PartialEq, Eq",
    "attr.rerun.scope": "blueprint",
) {
    /// Whether the force is enabled.
    enabled: bool (order: 100);

    /// The `y` position to pull nodes towards.
    y: double (order: 200);

    /// The strength of the force.
    strength: double (order: 300);
}
