    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug 'rerun' data.rrd",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--package=rerun"
                ],
                "filter": {
                    "name": "rerun",
                    "kind": "bin"
                }
            },
            "args": [
                "../data.rrd"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug 'rerun' objectron.rrd",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--package=rerun"
                ],
                "filter": {
                    "name": "rerun",
                    "kind": "bin"
                }
            },
            "args": [
                "../objectron.rrd"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug re_renderer sample",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--example=multiview"
                ],
                "filter": {
                    "name": "multiview",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
}
